Package: FUSION
Type: Package
Title: FUSION - Family-level Unique Small ncRNA IntegratION
Version: 0.1.0
Authors@R: c(
    person("Hukam", "Rawal", role = c("aut", "cre"), email = "hukam.rawal@gmail.com", comment = c(ORCID = "0000-0002-2727-9721")),
